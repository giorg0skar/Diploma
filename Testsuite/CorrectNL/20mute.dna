def main

  var x is int
   
  def f is int: n as int
    var s i is int
  begin
    s := 0
    i := 1
    loop:
    begin
      if i > n:
      begin
        break
      end       
      s := s + i
      i := i + 1
    end
    return: s
  end

begin
   x := 6
   x := f(x) + f(f(3))
   writeInteger: x
   writeString: "\n"
end
