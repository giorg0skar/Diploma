def main
   
   def fact is int: n as int
   begin
      if n = 0:
      begin
        return: 1
      end
      else:
      begin
        return: n * fact (n - 1)
      end
   end

   var z is int

begin
  z := fact(7)
  if z = 5040:
  begin
     writeString: "42\n"
  end
  else:
  begin
     writeInteger: z
     writeString: " wrong!\n"
  end
end
